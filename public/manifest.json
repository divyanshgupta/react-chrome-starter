{
  "name": "ATP WebSocket Inspector",
  "version":"1.0",
  "permissions": [
    "debugger",
    "tabs"
  ],
  "browser_action": {
    "default_title": "ATP WebSocket Frame Inspector"
  },
  "background": {
    "scripts": ["thirdParty/jquery-3.3.1.min.js","background.js"]
  },
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
     "128": "icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}